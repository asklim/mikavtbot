{
    "name": "mikavtbot",
    "version": "0.1.0",
    "description": "Telegram bot for usage in rsis",
    "main": "./dist/server/",
    "_moduleAliases": {
        "@": "dist",
        "<root>": "dist",
        "<srv>": "dist/server"
    },
    "private": true,
    "scripts": {
        "production": "NODE_ENV=production npm run start",
        "build": "npm run compile:clean",
        "start": "node --trace-warnings dist/server/",
        "start:tsc": "npm run compile && npm run start",
        "compile": "npx tsc",
        "compile:clean": "rm -rf dist/ && npm run compile",
        "debug": "DEBUG=$DEBUG,helpers:*,raw:*,api:* npm run debug:tbot",
        "debug:tbot": "DEBUG=$DEBUG,tbot*,actions:*,telegraf:main npm run start",
        "debug:all": "DEBUG=* npm run start",
        "debug:ts": "DEBUG=helpers:*,raw:*,api:*,app:*,tbot:*,actions:*,telegraf:main npm run start",
        "debug:tsc": "npm run compile && npm run debug:ts",
        "lint": "npx eslint .",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
        "test": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "mikavbot@asklim.github.com"
    },
    "author": "AsKlim",
    "license": "ISC",
    "dependencies": {
        "asklim": "git+https://asklim@github.com/asklim/asklim.git",
        "axios": "^1.4.0",
        "canvas": "^2.11.0",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.5",
        "debug": "^4.3.4",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.6",
        "envalid": "^7.3.1",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "http-errors": "^2.0.0",
        "jsonwebtoken": "^9.0.0",
        "loglevel": "^1.8.1",
        "module-alias": "^2.2.2",
        "mongoose": "^7.4.3",
        "morgan": "^1.10.0",
        "sharp": "^0.32.4",
        "telegraf": "^4.12.2",
        "yaml": "^2.3.1"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.17",
        "@types/http-errors": "^2.0.1",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/module-alias": "^2.0.1",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.5.0",
        "@types/node-fetch": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "^6.3.0",
        "@typescript-eslint/parser": "^6.3.0",
        "eslint": "^8.47.0",
        "jest": "^29.6.2",
        "source-map-loader": "^4.0.1",
        "ts-loader": "^9.4.2",
        "typegram": "^5.0.0",
        "typescript": "^5.1.6"
    }
}
