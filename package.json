{
    "name": "mikavtbot",
    "version": "0.23.1227",
    "description": "Telegram bot for usage in rsis",
    "main": "./dist/server/",
    "_moduleAliases": {
        "@": "dist",
        "<root>": "dist",
        "<srv>": "dist/server"
    },
    "private": true,
    "scripts": {
        "production": "NODE_ENV=production npm run start",
        "build": "npm run compile:clean",
        "start": "node --trace-warnings dist/server/",
        "start:tsc": "npm run compile && npm run start",
        "compile": "npx tsc",
        "compile:clean": "rm -rf dist/ && npm run compile",
        "debug": "DEBUG=$DEBUG,helpers:*,raw:*,api:* npm run debug:tbot",
        "debug:tbot": "DEBUG=$DEBUG,tbot*,actions:*,telegraf:main npm run start",
        "debug:all": "DEBUG=* npm run start",
        "debug:ts": "DEBUG=helpers:*,raw:*,api:*,app:*,tbot:*,actions:*,telegraf:main npm run start",
        "debug:ts/c": "npm run compile && npm run debug:ts",
        "lint": "npx eslint .",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
        "install:asklim": "npm install git+https://asklim@github.com/asklim/asklim.git",
        "test": "jest .",
        "spec": "jest 'spec/(.*)/asklim.spec.ts'",
        "spec:all": "jest 'spec/*/?(.*)+spec.[tj]s'"
    },
    "repository": {
        "type": "git",
        "url": "mikavbot@asklim.github.com"
    },
    "author": "AsKlim",
    "license": "ISC",
    "dependencies": {
        "asklim": "git+https://asklim@github.com/asklim/asklim.git",
        "axios": "^1.6.1",
        "canvas": "^2.11.0",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.5",
        "debug": "^4.3.4",
        "dotenv": "^16.3.1",
        "ejs": "^3.1.6",
        "envalid": "^8.0.0",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "http-errors": "^2.0.0",
        "imapflow": "^1.0.147",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "loglevel": "^1.8.1",
        "module-alias": "^2.2.2",
        "mongoose": "^8.0.0",
        "morgan": "^1.10.0",
        "nodemailer": "^6.9.7",
        "sharp": "^0.33.0",
        "telegraf": "^4.15.3",
        "yaml": "^2.3.4"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.6",
        "@types/debug": "^4.1.12",
        "@types/express": "^4.17.21",
        "@types/http-errors": "^2.0.4",
        "@types/jest": "^29.5.8",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/module-alias": "^2.0.4",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.10.4",
        "@types/node-fetch": "^2.6.9",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "eslint": "^8.53.0",
        "jest": "^29.7.0",
        "source-map-loader": "^4.0.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.5.0",
        "typegram": "^5.2.0",
        "typescript": "^5.2.2"
    }
}
