{
    "name": "mikavtbot",
    "version": "0.0.7",
    "description": "Telegram bot for usage in rsis",
    "main": "dist/index.js",
    "private": true,
    "scripts": {
        "production": "NODE_ENV=production npm run start",
        "start": "node --trace-warnings server/",
        "start:ts": "node --trace-warnings dist/",
        "start:tsc": "npx tsc && node --trace-warnings dist/",
        "debug": "DEBUG=$DEBUG,helpers:*,raw:*,api:* npm run debug:tbot",
        "debug:tbot": "DEBUG=$DEBUG,tbot*,actions:*,telegraf:main npm run start:ts",
        "debug:all": "DEBUG=* npm run start",
        "debug:tsc": "DEBUG=helpers:*,raw:*,api:*,tbot:*,actions:*,telegraf:main npm run start:tsc",
        "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
        "test": "npm run test"
    },
    "repository": {
        "type": "git",
        "url": "mikavbot@asklim.github.com"
    },
    "author": "AsKlim",
    "license": "ISC",
    "dependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^18.14.0",
        "axios": "^1.3.4",
        "bent": "^7.3.0",
        "colors": "^1.4.0",
        "cookie-parser": "^1.4.5",
        "debug": "*",
        "dotenv": "^10.0.0",
        "dotenv-expand": "^5.1.0",
        "ejs": "^3.1.6",
        "express": "^4.18.2",
        "form-data": "^4.0.0",
        "http-errors": "^1.8.1",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^6.10.0",
        "morgan": "^1.10.0",
        "telegraf": "^4.4.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.3",
        "@babel/preset-env": "^7.16.4",
        "@babel/preset-typescript": "^7.16.5",
        "eslint": "^8.3.0",
        "jest": "^27.4.3",
        "source-map-loader": "^4.0.1",
        "ts-loader": "^9.4.2",
        "ts-migrate": "^0.1.35",
        "typescript": "^4.9.5"
    }
}
